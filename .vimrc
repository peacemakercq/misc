set hlsearch
set incsearch
set magic
set showmatch
set mat=3

syntax enable

try
  colorscheme desert
catch
entry

set background=dark

set smartcase

set ignorecase

set encoding=utf8

set nobackup
set nowb
set noswapfile

set expandtab
set smarttab

set shiftwidth=4
set tabstop=4

set ai
set si
set wrap
